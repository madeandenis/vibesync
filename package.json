{
  "name": "@vibe-sync/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "cross-env npm run typeorm migration:run -- -d ./db/typeorm.config.ts",
    "migration:generate": "cross-env npm run typeorm -- migration:generate ./db/migrations/$npm_config_name -d ./db/typeorm.config.ts",
    "migration:create": "cross-env npm run typeorm -- migration:create ./db/migrations/$npm_config_name",
    "migration:revert": "cross-env npm run typeorm migration:revert -- -d ./db/typeorm.config.ts",
    "migration:show": "cross-env npm run typeorm migration:show -- -d ./db/typeorm.config.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.2",
